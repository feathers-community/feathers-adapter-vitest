{
  "name": "feathers-adapter-vitest",
  "version": "0.0.1",
  "description": "Feathers shared database adapter test suite with vitest",
  "homepage": "https://feathersjs.com",
  "keywords": [
    "feathers"
  ],
  "license": "MIT",
  "type": "module",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/feathers"
  },
  "engines": {
    "node": ">= 22"
  },
  "main": "dist/",
  "types": "dist/",
