  },
  "devDependencies": {
    "@feathers-community/eslint-config": "^0.0.8",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vitest": "^3.0.0"
  }
